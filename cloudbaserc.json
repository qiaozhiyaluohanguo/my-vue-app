{
  "envId": "my-vue-app-1gutshpt7c610c25",
  "framework": {
    "name": "vue-flask",
    "plugins": {
      "server": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": ".",
          "functions": [
            {
              "name": "flask_app",
              "timeout": 5,
              "envVariables": {},
              "runtime": "Python3.6",
              "installDependency": true,
              "handler": "flask_app.main"
            }
          ],
          "servicePaths": {
            "flask_app": "/api"
          }
        }
      },
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "npm run build",
          "outputPath": "dist",
          "ignore": [
            "node_modules",
            "cloudbaserc.json"
          ]
        }
      }
    }
  }
}
