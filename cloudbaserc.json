{
  "envId": "my-vue-app-1gutshpt7c610c25",
  "framework": {
    "name": "vue-flask",
    "plugins": {
      "server": {
        "use": "@cloudbase/framework-plugin-function",
        "inputs": {
          "functionRootPath": ".",
          "functionName": "flask-app",
          "runtime": "Python3.6",
          "installDependency": true,
          "apigatewayConf": {
            "protocols": ["http"],
            "environment": "release"
          }
        }
      },
      "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "npm run build",
          "outputPath": "dist",
          "ignore": [
            "node_modules",
            "cloudbaserc.json"
          ]
        }
      }
    }
  }
}
